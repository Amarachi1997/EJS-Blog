<%- include("partials/header"); -%>

<h1><%= pageTitle %></h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="newtitle">Title</label>
        <input type="text" name="newTitle" class="form-control">
        <label for="newPost">Post</label>
        <textarea name="newPost" cols="40" rows="5" class="form-control"></textarea>
        
    </div>
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>


<%- include("partials/footer"); -%>